{
  "manifest_version": 2,
  "name": "Roll20 Character Parser - DnD 5th SRD",
  "version": "0.1",
  "content_scripts" : [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.2.0.min.js", "getPageSource.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs", "<all_urls>"]
}
